<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apps.fitwell">

    <application
        android:allowBackup="true"
        android:icon="@drawable/fit_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/fit_logo"
        android:supportsRtl="false"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.CalculatorResultActivity"></activity>
        <activity android:name=".Activities.CalculatorActivity" />
        <activity android:name=".Activities.HistoryActivity" />
        <activity android:name=".Activities.EditProfileActivity" />
        <activity android:name=".Activities.SettingsActivity" />
        <activity android:name=".Activities.WorkoutsActivity" />
        <activity android:name=".Activities.WorkoutDetailsActivity" />
        <activity android:name=".Activities.RegisterActivity" />
        <activity android:name=".Activities.LoginActivity" />
        <activity android:name=".Activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.MainActivity" />
    </application>

</manifest>